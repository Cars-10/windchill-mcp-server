# Windchill MCP Server Configuration
# Copy this file to .env and update with your Windchill server details

# Windchill Server Configuration
WINDCHILL_URL=http://plm.windchill.com/Windchill
WINDCHILL_USER=wcadmin
WINDCHILL_PASSWORD=wcadmin

# MCP Server Configuration
MCP_SERVER_NAME=windchill-mcp
MCP_SERVER_VERSION=1.0.0
MCP_SERVER_PORT=3000

# Logging Configuration
LOG_LEVEL=info

# Optional: Network Configuration
# Uncomment if you need specific network settings
# WINDCHILL_TIMEOUT=30000
# WINDCHILL_MAX_RETRIES=3